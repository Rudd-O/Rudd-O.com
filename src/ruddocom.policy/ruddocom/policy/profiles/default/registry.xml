      <element>large 768:768</element>
      <element>preview 400:400</element>
      <element>mini 200:200</element>
      <element>thumb 128:128</element>
      <element>tile 64:64</element>
      <element>icon 32:32</element>
      <element>listing 16:16</element>
    </value>
